{
  "name": "some-awesome-newsletter-platform",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "publint": "^0.3.2",
    "sass-embedded": "^1.85.1",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@sveltejs/adapter-auto": "^4.0.0",
    "aos": "^2.3.4",
    "autoprefixer": "^10.4.20",
    "canvas-preset": "^3.3.8",
    "chart.js": "^4.4.7",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "gapi-script": "^1.2.0",
    "gsap": "^3.12.7",
    "hls.js": "^1.5.20",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.0",
    "postcss": "^8.5.1",
    "scene-preset": "^2.4.19",
    "svelte-forms-lib": "^2.0.1",
    "tailwindcss": "^4.0.3",
    "three": "^0.173.0",
    "victor": "^1.1.0"
  },
  "keywords": [
    "svelte"
  ]
}
